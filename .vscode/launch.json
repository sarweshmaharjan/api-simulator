{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch API Simulation",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/main.go",
      "env": {},
      "args": []
    },
    {
      "name": "Attach to Docker-Compose",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "/app",
      "port": 9990,
      "host": "localhost",
      "trace": "verbose"
    }
  ]
}
